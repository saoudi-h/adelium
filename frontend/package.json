{
    "name": "adelium",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "start:profile": "ng serve --configuration",
        "build": "ng build",
        "build:profile": "ng build --configuration",
        "build:docker": "ng build --configuration=docker",
        "build:ssl": "ng build --configuration=ssl",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "lint": "ng lint",
        "format": "npx prettier . -c --ignore-path .gitignore",
        "format:fix": "npx prettier . -w --ignore-path .gitignore",
        "storybook": "ng run adelium:storybook",
        "build-storybook": "ng run adelium:build-storybook",
        "compodoc": "npx compodoc -p tsconfig.doc.json -w -s --port 2222"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^17.0.4",
        "@angular/cdk": "^17.0.1",
        "@angular/common": "^17.0.4",
        "@angular/compiler": "^17.0.4",
        "@angular/core": "^17.0.4",
        "@angular/forms": "^17.0.4",
        "@angular/material": "^17.0.1",
        "@angular/platform-browser": "^17.0.4",
        "@angular/platform-browser-dynamic": "^17.0.4",
        "@angular/router": "^17.0.4",
        "@angular/service-worker": "^17.0.4",
        "@ng-select/ng-select": "^12.0.4",
        "@ngrx/effects": "^17.0.1",
        "@ngrx/entity": "^17.0.1",
        "@ngrx/router-store": "^17.0.1",
        "@ngrx/store": "^17.0.1",
        "@ngrx/store-devtools": "^17.0.1",
        "date-fns": "^2.30.0",
        "ngx-toastr": "^18.0.0",
        "rxjs": "~7.8.0",
        "tslib": "^2.3.0",
        "uuid": "^9.0.1",
        "zone.js": "~0.14.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^17.0.0",
        "@angular-eslint/builder": "17.1.0",
        "@angular-eslint/eslint-plugin": "17.1.0",
        "@angular-eslint/eslint-plugin-template": "17.1.0",
        "@angular-eslint/schematics": "17.1.0",
        "@angular-eslint/template-parser": "17.1.0",
        "@angular/compiler-cli": "^17.0.4",
        "@angular/language-service": "^17.0.4",
        "@compodoc/compodoc": "^1.1.22",
        "@ngrx/eslint-plugin": "^17.0.1",
        "@ngrx/schematics": "^17.0.1",
        "@storybook/addon-docs": "^7.5.1",
        "@storybook/addon-essentials": "^7.5.1",
        "@storybook/addon-interactions": "^7.5.1",
        "@storybook/addon-links": "^7.5.1",
        "@storybook/angular": "^7.5.1",
        "@storybook/blocks": "^7.5.1",
        "@storybook/testing-library": "^0.2.2",
        "@tailwindcss/typography": "^0.5.10",
        "@types/jasmine": "~4.3.0",
        "@types/uuid": "^9.0.7",
        "@typescript-eslint/eslint-plugin": "^6.10.0",
        "@typescript-eslint/parser": "^6.10.0",
        "autoprefixer": "^10.4.16",
        "daisyui": "^4.4.2",
        "eslint": "^8.54.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.1",
        "eslint-plugin-storybook": "^0.6.15",
        "jasmine-core": "~4.6.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "lint-staged": "^14.0.1",
        "postcss": "^8.4.31",
        "prettier": "^3.1.0",
        "prettier-eslint": "^15.0.1",
        "prettier-plugin-organize-imports": "^3.2.3",
        "prettier-plugin-tailwindcss": "^0.5.6",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "storybook": "^7.5.1",
        "tailwindcss": "^3.3.5",
        "typescript": "^5.2.2"
    },
    "lint-staged": {
        "*.{ts,html}": [
            "eslint --quiet --fix"
        ],
        "*.{json,md,html,ts}": [
            "prettier --write"
        ]
    }
}
